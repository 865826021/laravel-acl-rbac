{
    "name": "volicon/laravel-acl-rbac",
    "description": "ACL / RBAC - package for Laravel 5.  Can work with Models or routes",
    "authors": [
        {
            "name": "Nadav Vinik",
            "email": "nadav.v@volicon.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "illuminate/support": "5.*"
    },
	"require-dev": {
        "phpunit/phpunit": "3.7.*",
        "orchestra/testbench": "2.0.*",
		"mockery/mockery": "dev-master@dev"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Volicon\\Acl\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
